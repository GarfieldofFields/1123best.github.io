<script type="text/javascript">

  var itemCode = "zTuikt";
	var url = "https://xfl.jp/"+itemCode;



function getLinks(){

    var idItem        = "sara-tppmxa-uwlj-fqlp-p5if-dxgx62vm";

   FW.rest("Sara/"+idItem, "GET").done(function(data) {

    for (i in data.data.Files){
      if(data.data.Files[i].Profile=="risky"){
        $(".file-"+data.data.Files[i].Sara_File__).attr("href",data.data.Files[i].Download_Url);
        $(".viewfile-"+data.data.Files[i].Sara_File__).attr("href",data.data.Files[i].View_Url);
        $(".top-download").attr("href",data.data.Files[i].Download_Url);
      }else{
        $(".top-download").attr("href",data.data.Files[i].Download_Url);
      }
    }

    }).fail(function(data) {
          
    var token_error = data.responseJSON.token;

    });
}


$(document).ready(function()
    {

      getLinks();
      // count
      var countNum = 1;
      // Boolean Array
      var itemBooleanArray = [];
      var image_variation = "format=jpeg&strip&scale=1920x1080";

      // Check if Media_Image is generated.
      var imageCheck = function () {
        FW.rest("Sara/"+ "zTuikt", "GET", {image_variation:image_variation}).done(function(data) {
          var filesArray = data.data.Files;

          filesArray.forEach(function ( value, index) {
            var probesNum = Object.keys(value.Probes).length;
            var mediaImage = value.Media_Image;

              // The number of Probes is one & Media_Image null
              if (probesNum === 1 && mediaImage == void 0) {
                  itemBooleanArray[index] = false
                  return false;
              }
              else {
                  itemBooleanArray[index] = true
              }
          });


          $('.js-item-list').find('img').each(function (e) {
            var saraFileId = $(this).closest('[data-id]').attr('data-id');

            var currentItem = filesArray.filter( function( value ) {
              return value.Sara_File__ === saraFileId;
            });


            if (currentItem[0].Media_Image) {
              var itemVariationUrl = currentItem[0].Media_Image.Variation[image_variation];
              
              $('[data-id="'+ saraFileId +'"]').find('img').attr('src', itemVariationUrl);
            }
          });


          // If itemBooleanArray contains false 
          if (itemBooleanArray.indexOf(false) !== -1) {
            if (countNum <= 30) {
              setTimeout(function (){
                imageCheck();
              }, 2000 * countNum);
              countNum += 1;
            }
          }
        });
      }
      
      imageCheck();

        $('.modal').modal();
        $(".url").prepend(url);
        var subject = encodeURI("Inappropriate content");
        var message = encodeURI("Hello,\nI think these files are inappropriate. Here is the link :\n" + url);
        $(".link-report").attr("href", FW.prefix + "/contact?s=" + subject + "&m=" + message);

        $(".share_telegram").attr("href", "https://telegram.me/share/url?url=" + url + "&text=");
        $(".share_line").attr("href", "https://lineit.line.me/share/ui?url=" + url);
        $(".share_fb").attr("href", "fb-messenger://share/?link=" + url + "&app_id=198740224000638");
        $(".share_skype").attr("href", "https://web.skype.com/share?url=" + url + "&text=");
        $(".share_reddit").attr("href", "https://reddit.com/submit?url=" + url + "&title=");


        $("#cancel-modal-agree").click(function(){
            $("#delete-error").hide();
            $(this).prop("disable",true);
            FW.rest("Sara/sara-tppmxa-uwlj-fqlp-p5if-dxgx62vm", "DELETE").done(function(){
                location.href = FW.prefix+"/";
            }).fail(function(){
                $(this).prop("disable",false);
                $("#delete-error").show();
            });

        });
    });

new Clipboard('.copy-link', {
  text: function() {
  $(".copied-link").removeClass("hide");
  $(".copy-link").addClass("hide");
  setTimeout(function() {
    $(".copied-link").addClass("hide");
    $(".copy-link").removeClass("hide");
  }, 5000);
  

    return url;
  }
});

function downloadItemZip(){

  var idItem        = "sara-tppmxa-uwlj-fqlp-p5if-dxgx62vm";

  FW.rest("Sara/"+idItem+":makeZip", "POST").done(function(data) {


      var iframe;

      iframe = document.createElement('iframe');
      iframe.src = data.data.redirect_url;
      iframe.style.display = 'none';
      document.body.appendChild(iframe);

    }).fail(function(data) {
          
    var token_error = data.responseJSON.token;

    });
}

